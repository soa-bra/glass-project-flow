{
  "design-tokens": {
    "glassmorphism": {
      "background": "rgba(255,255,255,0.4)",
      "blur": {
        "backdropFilter": "blur(20px)",
        "webkitBackdropFilter": "blur(20px)"
      },
      "border": "1px solid rgba(255,255,255,0.2)",
      "shadow": "0 25px 50px -12px rgba(0,0,0,0.25)"
    },
    
    "dimensions": {
      "modal": {
        "sm": {
          "maxWidth": "max-w-md",
          "maxHeight": "max-h-[80vh]",
          "borderRadius": "rounded-[18px]",
          "padding": "p-4"
        },
        "md": {
          "maxWidth": "max-w-lg",
          "maxHeight": "max-h-[85vh]",
          "borderRadius": "rounded-[20px]",
          "padding": "p-5"
        },
        "lg": {
          "maxWidth": "max-w-2xl",
          "maxHeight": "max-h-[90vh]",
          "borderRadius": "rounded-[22px]",
          "padding": "p-6"
        },
        "xl": {
          "maxWidth": "max-w-3xl",
          "maxHeight": "max-h-[90vh]",
          "borderRadius": "rounded-[24px]",
          "padding": "p-6"
        }
      },
      "elements": {
        "borderRadiusFull": "rounded-3xl",
        "borderRadiusCircle": "rounded-full",
        "borderRadiusButton": "rounded-full",
        "borderRadiusInput": "rounded-3xl"
      }
    },
    
    "header": {
      "layout": "flex items-center justify-between mb-6",
      "icon": {
        "size": "w-10 h-10",
        "background": "bg-black",
        "borderRadius": "rounded-full",
        "iconColor": "text-white",
        "iconSize": "w-5 h-5"
      },
      "title": {
        "fontSize": "text-xl",
        "fontWeight": "font-bold",
        "color": "text-black",
        "fontFamily": "font-arabic"
      },
      "closeButton": {
        "size": "w-8 h-8",
        "borderRadius": "rounded-full",
        "border": "border border-black",
        "background": "bg-transparent",
        "hover": "hover:bg-black/5",
        "position": "absolute top-4 left-4",
        "zIndex": "z-10"
      }
    },
    
    "interactive-elements": {
      "labels": {
        "fontWeight": "font-bold",
        "color": "text-black",
        "fontFamily": "font-arabic",
        "spacing": "mb-2"
      },
      
      "input-fields": {
        "borderRadius": "rounded-3xl",
        "background": "bg-white/30",
        "border": {
          "default": "border border-black/20",
          "focus": "focus:border-black focus:outline-none"
        },
        "padding": "px-4 py-3",
        "text": {
          "align": "text-right",
          "fontFamily": "font-arabic"
        },
        "placeholder": "placeholder-black/50",
        "transition": "transition-colors duration-200"
      },
      
      "textarea": {
        "borderRadius": "rounded-2xl",
        "background": "bg-white/30",
        "border": {
          "default": "border border-black/20",
          "focus": "focus:border-black focus:outline-none"
        },
        "padding": "px-4 py-3",
        "text": {
          "align": "text-right",
          "fontFamily": "font-arabic"
        },
        "placeholder": "placeholder-black/50",
        "transition": "transition-colors duration-200",
        "resize": "resize-none"
      },
      
      "select": {
        "borderRadius": "rounded-3xl",
        "background": "bg-white/30",
        "border": {
          "default": "border border-black/20",
          "focus": "focus:border-black focus:outline-none"
        },
        "padding": "px-4 py-3",
        "text": {
          "align": "text-right",
          "fontFamily": "font-arabic"
        },
        "transition": "transition-colors duration-200"
      },
      
      "date-picker": {
        "button": {
          "borderRadius": "rounded-3xl",
          "background": "bg-white/30",
          "border": {
            "default": "border border-black/20",
            "focus": "focus:border-black"
          },
          "padding": "px-4 py-3",
          "text": {
            "align": "text-right",
            "fontFamily": "font-arabic"
          },
          "icon": "w-4 h-4"
        },
        "popup": {
          "zIndex": "z-[10000]"
        }
      },
      
      "toggle-buttons": {
        "container": {
          "border": "border border-black/10",
          "borderRadius": "rounded-full",
          "padding": "p-1",
          "background": "bg-transparent"
        },
        "button": {
          "padding": "px-4 py-2",
          "borderRadius": "rounded-full",
          "fontSize": "text-sm",
          "fontWeight": "font-medium",
          "transition": "transition-colors duration-200",
          "fontFamily": "font-arabic"
        },
        "states": {
          "selected": {
            "background": "bg-black",
            "color": "text-white"
          },
          "unselected": {
            "color": "text-black",
            "hover": "hover:bg-black/5"
          }
        }
      }
    },
    
    "action-buttons": {
      "layout": "flex items-center justify-end gap-3 mt-6",
      "primary": {
        "background": {
          "default": "bg-black",
          "hover": "hover:bg-black/90"
        },
        "color": "text-white",
        "padding": "px-6 py-3",
        "borderRadius": "rounded-full",
        "fontFamily": "font-arabic",
        "fontWeight": "font-medium",
        "transition": "transition-colors duration-200",
        "disabled": "disabled:opacity-70 disabled:cursor-not-allowed"
      },
      "secondary": {
        "background": {
          "default": "bg-white/30",
          "hover": "hover:bg-white/40"
        },
        "border": "border border-black/20",
        "color": "text-black",
        "padding": "px-6 py-3",
        "borderRadius": "rounded-full",
        "fontFamily": "font-arabic",
        "fontWeight": "font-medium",
        "transition": "transition-colors duration-200"
      },
      "destructive": {
        "background": {
          "default": "bg-red-600",
          "hover": "hover:bg-red-700"
        },
        "color": "text-white",
        "padding": "px-6 py-3",
        "borderRadius": "rounded-full",
        "fontFamily": "font-arabic",
        "fontWeight": "font-medium",
        "transition": "transition-colors duration-200",
        "disabled": "disabled:opacity-70 disabled:cursor-not-allowed"
      }
    },
    
    "spacing": {
      "vertical": {
        "sections": "space-y-6",
        "fieldGroups": "space-y-4",
        "labels": "space-y-2"
      },
      "horizontal": {
        "elements": "gap-3",
        "buttons": "gap-3",
        "form": "gap-4"
      },
      "margins": {
        "section": "mb-6",
        "field": "mb-4",
        "label": "mb-2"
      },
      "borders": {
        "divider": "border-black/10"
      }
    },
    
    "typography": {
      "arabic": {
        "fontFamily": "font-arabic",
        "textAlign": "text-right"
      },
      "weights": {
        "regular": "font-normal",
        "medium": "font-medium",
        "semibold": "font-semibold",
        "bold": "font-bold"
      },
      "sizes": {
        "xs": "text-xs",
        "sm": "text-sm",
        "base": "text-base",
        "lg": "text-lg",
        "xl": "text-xl",
        "2xl": "text-2xl"
      }
    },
    
    "effects": {
      "transitions": {
        "colors": "transition-colors duration-200",
        "all": "transition-all duration-200",
        "transform": "transition-transform duration-200"
      },
      "animations": {
        "fadeIn": "animate-in fade-in-0 duration-200",
        "fadeOut": "animate-out fade-out-0 duration-200",
        "slideIn": "animate-in slide-in-from-bottom-4 duration-300",
        "slideOut": "animate-out slide-out-to-bottom-4 duration-300"
      },
      "zIndex": {
        "modal": "z-[9999]",
        "popup": "z-[10000]",
        "tooltip": "z-[10001]",
        "closeButton": "z-10"
      },
      "opacity": {
        "disabled": "opacity-70",
        "placeholder": "black/50",
        "background30": "white/30",
        "background40": "white/40",
        "border10": "black/10",
        "border20": "black/20",
        "hover5": "black/5"
      }
    },
    
    "semantic-colors": {
      "primary": "#000000",
      "primaryText": "#FFFFFF",
      "secondary": "rgba(255,255,255,0.3)",
      "secondaryText": "#000000",
      "success": "#22c55e",
      "successText": "#FFFFFF",
      "warning": "#f59e0b",
      "warningText": "#FFFFFF",
      "danger": "#dc2626",
      "dangerText": "#FFFFFF",
      "border": {
        "default": "rgba(0,0,0,0.1)",
        "light": "rgba(0,0,0,0.2)",
        "strong": "#000000"
      },
      "glass": {
        "background": "rgba(255,255,255,0.4)",
        "border": "rgba(255,255,255,0.2)"
      }
    },
    
    "backdrop": {
      "background": "rgba(255,255,255,0.3)",
      "blur": "backdrop-blur-sm"
    }
  },
  
  "component-mappings": {
    "modal-container": [
      "glassmorphism.background",
      "glassmorphism.blur",
      "glassmorphism.border",
      "glassmorphism.shadow",
      "dimensions.modal",
      "effects.zIndex.modal"
    ],
    
    "modal-backdrop": [
      "backdrop.background",
      "backdrop.blur",
      "effects.zIndex.modal"
    ],
    
    "input-field": [
      "interactive-elements.input-fields.borderRadius",
      "interactive-elements.input-fields.background",
      "interactive-elements.input-fields.border",
      "interactive-elements.input-fields.padding",
      "interactive-elements.input-fields.text",
      "interactive-elements.input-fields.placeholder",
      "interactive-elements.input-fields.transition"
    ],
    
    "textarea-field": [
      "interactive-elements.textarea.borderRadius",
      "interactive-elements.textarea.background",
      "interactive-elements.textarea.border",
      "interactive-elements.textarea.padding",
      "interactive-elements.textarea.text",
      "interactive-elements.textarea.placeholder",
      "interactive-elements.textarea.transition",
      "interactive-elements.textarea.resize"
    ],
    
    "select-field": [
      "interactive-elements.select.borderRadius",
      "interactive-elements.select.background",
      "interactive-elements.select.border",
      "interactive-elements.select.padding",
      "interactive-elements.select.text",
      "interactive-elements.select.transition"
    ],
    
    "toggle-group": [
      "interactive-elements.toggle-buttons.container",
      "interactive-elements.toggle-buttons.button",
      "interactive-elements.toggle-buttons.states"
    ],
    
    "action-button-primary": [
      "action-buttons.primary.background",
      "action-buttons.primary.color",
      "action-buttons.primary.padding",
      "action-buttons.primary.borderRadius",
      "action-buttons.primary.fontFamily",
      "action-buttons.primary.fontWeight",
      "action-buttons.primary.transition",
      "action-buttons.primary.disabled"
    ],
    
    "action-button-secondary": [
      "action-buttons.secondary.background",
      "action-buttons.secondary.border",
      "action-buttons.secondary.color",
      "action-buttons.secondary.padding",
      "action-buttons.secondary.borderRadius",
      "action-buttons.secondary.fontFamily",
      "action-buttons.secondary.fontWeight",
      "action-buttons.secondary.transition"
    ],
    
    "action-button-destructive": [
      "action-buttons.destructive.background",
      "action-buttons.destructive.color",
      "action-buttons.destructive.padding",
      "action-buttons.destructive.borderRadius",
      "action-buttons.destructive.fontFamily",
      "action-buttons.destructive.fontWeight",
      "action-buttons.destructive.transition",
      "action-buttons.destructive.disabled"
    ]
  }
}